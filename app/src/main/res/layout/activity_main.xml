<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity">

    <Button
        android:id="@+id/btInventaire"
        android:layout_width="153dp"
        android:layout_height="160dp"
        android:layout_marginStart="48dp"
        android:text="Inventaire"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.264" />

    <Button
        android:id="@+id/btTellecollecte"
        android:layout_width="153dp"
        android:layout_height="160dp"
        android:layout_marginEnd="44dp"
        android:text="Telecollecte"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/btCaisse"
        app:layout_constraintTop_toBottomOf="@+id/btParametre"
        app:layout_constraintVertical_bias="0.242" />

    <Button
        android:id="@+id/btCaisse"
        android:layout_width="153dp"
        android:layout_height="160dp"
        android:layout_marginStart="48dp"
        android:text="Caisse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btInventaire"
        app:layout_constraintVertical_bias="0.242" />

    <Button
        android:id="@+id/btParametre"
        android:layout_width="153dp"
        android:layout_height="160dp"
        android:layout_marginEnd="48dp"
        android:text="Parametre"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/btInventaire"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.264" />

</androidx.constraintlayout.widget.ConstraintLayout>